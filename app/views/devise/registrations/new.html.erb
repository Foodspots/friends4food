<div class="panel panel-default">
  <div class="panel-heading">
    <h1>Maak een account aan</h1>
  </div>


<div class="panel-heading">
  Gebruik je Facebook account
</div>
    <div class="panel-body">
     <%= link_to "Login met Facebook", user_omniauth_authorize_path(:facebook), class: "btn btn-info" %><br/>
    <br />
</div>

<div class="panel-heading">
  Of maak een Altijd Heerlijk account aan
  </div>


  <div class="panel-body">
    <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
      <%= devise_error_messages! %>

      <div class="form-group">
        <%= f.label :name, "Naam" %>
        <%= f.text_field :name, class: "form-control", :autofocus => true %>
      </div>

      <div class="form-group">
        <%= f.label :email, "Email adres" %>
        <%= f.email_field :email, autofocus: true, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :password, "Wachtwoord" %>
        <%= f.password_field :password, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.submit "Bewaar", class: "btn btn-info" %>
      </div>
    <% end %>
  </div>



  <div class="panel-footer">
    <%= render "devise/shared/links" %>
  </div>
</div>
<% if user_signed_in? %>  
<h1>Alle chefs</h1>
<p>Bekijk hier de <strong><%= @user.count %> chefs </strong> die lid zijn van Altijd Heerlijk. <br>
    <div id="pins" class="transitions-enabled">
      <% @user.each do |user| %>
        <div class="box panel panel-default" id = "user_<%= user.id %>">
            <%= render :partial => 'users/user', :locals => {:user => user} %>
        </div>
        <% end %>
    </div>
<% else %>
<%= render 'pages/login' %>
<% end %>


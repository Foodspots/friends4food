<div class="col-xs-14">
	<h3>Add friends</h3>
	<div class="panel panel-default">
<p><u>Tip</u>: Send an invitation to your Facebook contacts.<%= share_snippet %></p>
	</div>
    <div id="pins" class="transitions-enabled">
      <% @user.each do |user| %>
        <div class="box panel panel-default" id = "user_<%= user.id %>">
            <%= render :partial => 'users/user', :locals => {:user => user} %>
        </div>
        <% end %>
    </div>
 </div>





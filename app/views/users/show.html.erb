 <div class="col-xs-14">
 <h3><%= @user.name %>' favorite HotSpots</h3>
 <div class="panel panel-default">
 <p>View the favorite HotSpots of <%= @user.name %>.</p>
 
 <div id="follow_unfollow_btn" class="btn_margin">
   <%= render :partial => 'follow_unfollow_btn', :locals => {:user => @user} %>
 </div>
</div>
 <% @user.find_liked_items.each do |pin| %>
   <%= render 'pins/show_pin_details' %>  
 <% end %>

 </div>
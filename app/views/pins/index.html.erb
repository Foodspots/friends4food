<% if user_signed_in? %>  
<div id="pins" class="transitions-enabled">
  <% @pins.each do |pin| %>
    <div class="box panel panel-default">
          <div class="panel-body">
          <strong><p><%= pin.description %></p></p></strong>
          <%= link_to image_tag(pin.image.url(:medium)), pin %>
                <div class="actions">
                <p>Toegevoegd door: <strong><%= pin.user.name if pin.user %></p></strong>
                <p><%= link_to 'Bekijk recept', pin_path(pin) %></p>
              </div>
          </div>
    </div>
  <% end %>
</div>
<% else %>
<%= render 'pages/login' %>
<% end %>
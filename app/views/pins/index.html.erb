<% if user_signed_in? %>  
<div id="pins" class="transitions-enabled">
  <% @pins.each do |pin| %>
    <div class="box panel panel-default">
        <%= link_to image_tag(pin.image.url(:medium)), pin %>
          <div class="panel-body">
          <p><%= pin.description %></p>
                <div class="actions">
                <p><%= link_to 'Bekijk recept', pin_path(pin) %></p>
              </div>
          </div>
    </div>
  <% end %>
</div>
<% else %>
<%= render 'pages/login' %>
<% end %>